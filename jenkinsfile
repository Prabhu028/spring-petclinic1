pipeline{
    agent any
    stages{
        stage('scr'){
            steps{
                url:'https://github.com/Prabhu028/spring-petclinic1.git', 
                branch:'main'
            }
        }
        stage('build'){
            steps{
                sh 'mvn package'
                archiveArtifacts artifacts: '**/spring-petclinic-*.jar'
                junit testResults: '**/TEST-*.xml'
                
            }
        }

    }


}